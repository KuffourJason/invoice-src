add_type("nuts")
add_type("bolts")
add_type("hammers")
add_type("nuts")
add_product("bolts",100)
add_product("bolts",0)
add_product("bolts",-1)
add_product("junk",200)
add_product("nuts",1005)
add_order(<<["nuts", 29], ["bolts", 31]>>)
add_order(<<["hammers", 1]>>)
add_order(<<["nuts", 1]>>)
invoice(1)
invoice(2)
invoice(1)
cancel_order(1)
add_product("hammers",100)
add_order(<<["hammers", 2], ["nuts", 10]>>)
add_order(<<["hammers", 2], ["nuts", 10], ["bolts", 5]>>)

